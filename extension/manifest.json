{
	"version": "1.6",
	"name": "xkcd Subtext",
	"content_scripts": [
		{
			"matches": [
				"http://xkcd.com/*",
				"http://xkcd.org/*",
				"http://www.xkcd.com/*",
				"http://www.xkcd.org/*"
			],
			"js": ["scripts/xkcds.js"],
			"run_at": "document_end"
		}
	],
	"icons": {
		"16": "images/icon16.png",
		"32": "images/icon32.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"options_page": "options.html",
	"background_page": "scripts/background.html"
}