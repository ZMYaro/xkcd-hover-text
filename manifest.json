{
	"update_url":"http://clients2.google.com/service/update2/crx",
	"version": "1.6",
	"name": "xkcd Subtext",
	"content_scripts": [
		{
			"matches": [
				"http://xkcd.com/*",
				"http://xkcd.org/*",
				"http://www.xkcd.com/*",
				"http://www.xkcd.org/*"
			],
			"js": ["xkcds.js"],
			"run_at": "document_end"
		}
	],
	"icons": { "16": "icon16.png",
           "32": "icon32.png",
           "48": "icon48.png",
          "128": "icon128.png" },
	"options_page": "options.html",
	"background_page": "background.html"
}